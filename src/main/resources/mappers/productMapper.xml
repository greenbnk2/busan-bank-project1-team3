<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.bnk.bnk_project.mapper.FundMapper">

    <select id="findProductList" resultType="kr.co.bnk.bnk_project.dto.ProductDTO">
        SELECT
            fundCode As fundCode,
            fundName AS fundName,
            perf1M   AS perf1M,
            perf3M   AS perf3M,
            perf6M   AS perf6M,
